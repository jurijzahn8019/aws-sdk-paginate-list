{
  "name": "aws-sdk-paginate-list",
  "version": "1.0.0",
  "description": "Provides a function to paginate over a list function on aws-sdk",
  "main": ".commitlintrc.js",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "tsc -w",
    "clean": "rm -rf tmp/* dist/*",
    "coveralls": "cat ./tmp/coverage/lcov.info | coveralls",
    "lint": "eslint src/**/*.ts",
    "packlocal": "export P=jurijzahn8019-aws; npm run clean && npm run build && npm pack && mkdir -p ./tmp && mv -f $P-*.tgz ./tmp/ && tar xfz ./tmp/$P-*.tgz -C ./tmp",
    "pub": "npm run clean && npm run build && npm publish",
    "release": "standard-version",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurijzahn8019/aws-sdk-paginate-list.git"
  },
  "keywords": [
    "aws",
    "aws-sdk",
    "list",
    "paginate"
  ],
  "author": "Jurij Zahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jurijzahn8019/aws-sdk-paginate-list/issues"
  },
  "homepage": "https://github.com/jurijzahn8019/aws-sdk-paginate-list#readme",
  "peerDependencies": {
    "aws-sdk": "^2.x.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@jurijzahn8019/aws-promise-jest-mock": "^2.1.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "aws-sdk": "^2.623.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "git-branch": "^2.0.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "prettier": "^1.19.1",
    "rollup": "^1.31.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  }
}
